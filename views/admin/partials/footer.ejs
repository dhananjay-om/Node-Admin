<script>
  // Check if there's a success or error message in the URL
  const urlParams = new URLSearchParams(window.location.search);
  const successMsg = urlParams.get('success_msg');
  const errorMsg = urlParams.get('error_msg');

  if (successMsg) {
      Swal.fire({
          position: "top-end",
          icon: "success",
          title: successMsg,
          showConfirmButton: false,
          timer: 1500
      }).then(() => {
          // Remove the success_msg parameter from the URL
          window.history.replaceState(null, null, window.location.pathname);
      });
  }

  if (errorMsg) {
      Swal.fire({
          position: "top-end",
          icon: "error",
          title: errorMsg,
          showConfirmButton: false,
          timer: 1500
      }).then(() => {
          // Remove the error_msg parameter from the URL
          window.history.replaceState(null, null, window.location.pathname);
      });
  }
</script>
